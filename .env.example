# S3-compatible storage endpoint URL (e.g., https://account.r2.cloudflarestorage.com)
S3_ENDPOINT=
# S3 access key ID
S3_ACCESS_KEY_ID=
# S3 secret access key
S3_SECRET_ACCESS_KEY=
# S3 bucket name
S3_BUCKET_NAME=
# S3 region (use "auto" for Cloudflare R2)
S3_REGION=auto
# Use path-style URLs (true for MinIO, false for most services)
S3_FORCE_PATH_STYLE=false

# Meilisearch host for indexing
MEILISEARCH_HOST=http://localhost:7700
# Meilisearch master key
MEILISEARCH_MASTER_KEY=masterKey

# Meilisearch host for frontend search
VITE_MEILISEARCH_HOST=https://search.x.noz.one
# Meilisearch search-only API key
VITE_MEILISEARCH_DEFAULT_SEARCH_KEY=3266e98f431f6e28db4c804bd4d6b95fc43c8510a693969ef76d5ec3ec62a204
